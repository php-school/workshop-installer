{
    "name": "php-school/workshop-installer",
    "description": "Installer for PHP School workshops",
    "type": "composer-plugin",
    "require": {
        "php" : ">=5.6",
        "composer-plugin-api": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0",
        "squizlabs/php_codesniffer": "^2.3",
        "composer/composer": "~1.0@dev"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Aydin Hassan",
            "email": "aydin@hotmail.co.uk"
        }
    ],
    "autoload": {
        "psr-4": {
            "PhpSchool\\PhpWorkshopInstaller\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhpSchool\\PhpWorkshopInstaller\\": "test/"
        }
    },
    "extra": {
        "class": "\\PhpSchool\\PhpWorkshopInstaller\\Plugin"
    }
}
